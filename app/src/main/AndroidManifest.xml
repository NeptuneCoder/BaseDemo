<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.xiaoyun.guide">

    <application
        android:name=".GuideApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/ThemeActivity"
        >
        <activity android:name=".moudles.main.MainActivity" />
        <activity android:name=".moudles.guide.GuideActivity" />
        <activity android:name=".moudles.launch.LaunchActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".moudles.login.LoginActivity"></activity>
    </application>

</manifest>